@model Department
@{
	ViewData["Title"] = "Details";
}

<h1>Department Details</h1>
<br />
<br />
<div class="form-group">
    <label asp-for="ID" class="col-form-label"></label>
    <input readonly asp-for="ID" class="form-control" readonly />
    <span asp-validation-for="ID" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Dep_name" class="col-form-label"></label>
    <input readonly asp-for="Dep_name" class="form-control" readonly />
    <span asp-validation-for="Dep_name" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="establish_date" class="col-form-label"></label>
    <input readonly asp-for="establish_date" class="form-control" readonly />
</div>
<div class="form-group">
    <a asp-controller="Department" asp-action="Edit" class="btn btn-warning">Edit</a>
    <a asp-controller="Department" asp-action="getallview" class="btn btn-light">Back to List</a>
</div>

