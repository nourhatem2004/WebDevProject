@model Department
@{
	ViewData["Title"] = "Edit";
}

<h1>Edit Department</h1>
<br />
<br />
<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="Dep_name" class="col-form-label"></label>
        <input asp-for="Dep_name" class="form-control" />
        <span asp-validation-for="Dep_name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="establish_date" class="col-form-label"></label>
        <input asp-for="establish_date" class="form-control" />
        <span asp-validation-for="establish_date" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
        <a asp-controller="Department" asp-action="getallview" class="btn btn-light">Back to List</a>
    </div>
</form>